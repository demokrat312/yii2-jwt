{
    "name": "demokrat312/yii2-jwt",
    "description": "Copy of sizeg/yii2-jwt",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "jwt"],
    "authors": [
        {
            "name": "Oleg Gismatulin",
            "email": "kampo3000@gmail.com",
            "homepage": "https://google.com"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "lcobucci/jwt": "~4.2.1",
        "yiisoft/yii2": "~2.0.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "sizeg\\jwt\\": "src"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "sizeg\\jwt\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
